{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": ["astro/client"],
		"baseUrl": ".",
		"paths": {
			"@globals": ["src/components/global"],
			"@component/*": ["src/components/*"],
		}
	}
}
